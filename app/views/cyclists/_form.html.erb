<%= form_for @cyclist do |f| %>
  <div>
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </div>
  <div>
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name %>
  </div>
  <div>
    <%= f.label :photo %><br>
    <%= f.text_field :photo %>
  </div>
  <div>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %><br>
    <%= f.text_field :password %>
  </div>
  <div>
    <%= f.label :bio %><br>
    <%= f.text_field :bio %>
  </div>
  <div>
    <%= f.submit "Submit" %>
  </div>
<% end %>